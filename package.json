{
  "name": "modern-calendar",
  "version": "2.1.3",
  "description": "A calendar module for displaying events.",
  "main": "dist/calendar.umd.js",
  "exports": {
    ".": {
      "import": "./dist/calendar.es.js",
      "require": "./dist/calendar.cjs.js"
    }
  },
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "build:css": "postcss src/styles/tailwind.css -o dist/styles.css"
  },
  "keywords": [
    "calendar",
    "events",
    "mithril"
  ],
  "author": "ADANDE Appolinaire",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^10.4.19",
    "mithril": "^2.2.11",
    "postcss": "^8.4.39",
    "postcss-cli": "^11.0.0",
    "vite": "^6.0.1"
  },
  "devDependencies": {
    "css-loader": "^7.1.2",
    "eslint": "^9.7.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-mithril": "^0.2.0",
    "mini-css-extract-plugin": "^2.9.0",
    "style-loader": "^4.0.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  }
}
